<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" 
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="jimchen.InsertFrameGUIController">
    
    <HBox spacing="10" alignment="CENTER" GridPane.columnIndex="0" 
          GridPane.rowIndex="1" AnchorPane.topAnchor="350" AnchorPane.leftAnchor="200"  > 
        
        <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
        </padding>
        
        <Button text="Cancel" fx:id="closeButton" prefWidth="80" textAlignment="CENTER" alignment="CENTER"  style="-fx-font: NORMAL 14 Tahoma;" onAction="#cancel" />
        
        <Button text="Save" fx:id= "saveButton" prefWidth="80" textAlignment="CENTER" alignment="CENTER"  style="-fx-font: NORMAL 14 Tahoma;" onAction="#save" />


    </HBox>
    
</AnchorPane>
